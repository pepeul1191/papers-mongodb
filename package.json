{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "rollup -c -w & node ./node_modules/nodemon/bin/nodemon.js node ./bin/www --ignore './public/' --ignore './src/'",
    "dev": "rollup -c -w & node ./node_modules/nodemon/bin/nodemon.js node app.js --ignore './public/' --ignore './src/'",
    "dev:server": "node ./node_modules/nodemon/bin/nodemon.js node app.js --ignore './public/' --ignore './src/'",
    "dev:front": "rollup -c -w",
    "bower:install": "node ./node_modules/.bin/bower install",
    "build": "rollup -c",
    "dbmate:new": "npx dbmate -d \"db/migrations\" -e \"DB\" new ",
    "dbmate:up": "npx dbmate -d \"db/migrations\" -e \"DB\" up",
    "dbmate:rollback": "npx dbmate -d \"db/migrations\" -e \"DB\" rollback"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.2.3",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "font-awesome": "^4.7.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^9.1.0",
    "http-errors": "~1.6.3",
    "js-yaml": "^4.0.0",
    "mongoose": "^8.4.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.5.0",
    "query-string": "^7.0.1",
    "sirv-cli": "^2.0.0",
    "socket.io": "^4.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "bower": "^1.8.12",
    "dbmate": "^2.5.0",
    "nodemon": "^2.0.7",
    "rollup": "^3.15.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte": "^3.55.0",
    "svelte-routing": "^1.6.0"
  }
}
